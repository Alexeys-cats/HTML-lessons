{
  "name": "html-lessons",
  "private": false,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "concurrently \"npm run dev\" \"npm run server\"",
    "build": "tsc -b && vite build",
    "build:ci": "vite build",
    "build:prod": "cross-env BASE_PATH=/HTML-lessons/ vite build",
    "deploy:prod": "npm run build:prod && gh-pages -d dist -b gh-pages -r https://github.com/Alexeys-cats/HTML-lessons.git",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "sb": "storybook dev -p 6006",
    "build-sb": "storybook build",
    "server": "json-server json-server/db.json --port 5030"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.1.1",
    "@reduxjs/toolkit": "^2.8.2",
    "@tailwindcss/postcss": "^4.1.11",
    "clsx": "^2.1.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "framer-motion": "^12.23.6",
    "postcss": "^8.5.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.60.0",
    "react-icons": "^5.5.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.6.3",
    "storybook": "^9.0.17",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.0.1",
    "@eslint/css": "^0.9.0",
    "@eslint/json": "^0.13.0",
    "@eslint/markdown": "^6.6.0",
    "@storybook/addon-a11y": "^9.0.17",
    "@storybook/addon-actions": "^9.0.8",
    "@storybook/addon-docs": "^9.0.17",
    "@storybook/addon-vitest": "^9.0.17",
    "@storybook/react-vite": "^9.0.17",
    "@types/node": "^24.0.15",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "@vitejs/plugin-react": "^4.5.2",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "concurrently": "^9.2.0",
    "eslint": "^9.30.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-storybook": "^9.0.17",
    "globals": "^16.2.0",
    "json-server": "^1.0.0-beta.3",
    "playwright": "^1.54.1",
    "prettier": "^3.6.2",
    "strip-ansi": "^7.1.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.0",
    "vitest": "^3.2.4"
  }
}
